{
  "version": 2,
  "builds": [
    {
      "src": "backend/simple_server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/simple_server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DOMAIN": "propvet.taurusai.io"
  }
}